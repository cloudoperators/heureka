{
  "branches": [
    {"name": "main", "channel": "latest", "initial": "0.1.0"}
  ],
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/github",
      {
        "assets": [
          {
            "path": "dist/*.tar.gz",
            "label": "Binary distribution"
          }
        ]
      }
    ]
  ]
}